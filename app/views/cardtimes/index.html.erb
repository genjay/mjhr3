<h1>上鐘上傳_刷卡資料</h1>
 
<%= form_tag import_cardtimes_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import", data: { disable_with: "執行中" } %>
<% end %>


<table>
  <thead>
    <tr>
      <th>Ou</th>
      <th>Cardno</th>
      <th>Cardtime</th>
      <th>Up data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cardtimes.each_with_index do |cardtime,i| %>
      <tr> 
      	<td><%= i+1 %></td>
        <td><%= cardtime.up_data %></td>
      </tr>
    <% end %>
  </tbody>
</table>