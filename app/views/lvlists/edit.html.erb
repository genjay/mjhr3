<h2>級距表維護</h2>

<div id='rcorners2'>
  <%= simple_form_for @lvlist, url: lvtype_lvlist_path do |f| %>
  <% f.error_notification %>

  <div class="form-inputs"> 
    <%= f.input :uid,label:'代號' %>
    <%= f.input :amt,label:'金額' %>
  </div>

  <div class="form-actions">
      <%= f.button :submit,'存檔',class: 'btn btn-primary' %>
      <%= link_to '回上一頁', lvtype_lvlists_path,class: 'btn btn-warning', method: :get %>
  </div>

<% end %>
</div>
