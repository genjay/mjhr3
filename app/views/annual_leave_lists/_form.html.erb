<%= simple_form_for(@annual_leave_list) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :months_of_job %>
    <%= f.input :days %>
    <%= f.input :memo %>
    <%= f.input :ou_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
