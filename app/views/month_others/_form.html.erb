<%= simple_form_for(@month_other) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs"> 
    <%= f.association :employee %>
    <%= f.association :pay_type,include_blank: false  %>
    <%= f.input :yyyymm %>
    <%= f.input :amt %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
